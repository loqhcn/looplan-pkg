{
  "name": "looplan-ui",
  "version": "1.5.7",
  "description": "基于Vue3的UI组件库",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "cross-env BUILD_MODE=lib vite build && tsc --project tsconfig.build.json --emitDeclarationOnly && sass src/scss/index.scss lib/index.css",
    "build:sass": "sass src/scss/index.scss lib/index.css",
    "watch": "sass --watch src/scss/index.scss lib/index.css",
    "dev": "vite",
    "build:lib": "BUILD_MODE=lib vite build ",
    "build:types": "tsc --project tsconfig.build.json --emitDeclarationOnly",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build"
  },
  "dependencies": {
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loqhcn/looplan-ui"
  },
  "homepage": "项目主页",
  "bugs": {
    "url": "https://github.com/loqhcn/looplan-ui/issues"
  },
  "files": [
    "dist",
    "lib",
    "types",
    "README.md",
    "LICENSE"
  ],
  "main": "lib/looplan-ui.umd.js",
  "module": "lib/looplan-ui.es.js",
  "types": "types/index.d.ts",
  "yalcSig": "7fb0209a5d6ad550ee229eddfb569921"
}
