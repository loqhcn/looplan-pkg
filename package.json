{
  "name": "looplan-pkg",
  "private": false,
  "version": "2.0.0",
  "type": "module",
  "description": "现代化的 Vue 3 + TypeScript 组件库开发模板，支持多包管理和智能构建系统",
  "keywords": [
    "vue3",
    "typescript",
    "component-library",
    "vite",
    "monorepo",
    "ui-components",
    "bun"
  ],
  "scripts": {
    "dev": "vite",
    "build": "bun run ./scripts/build.ts",
    "build:pkg": "bun run ./scripts/build.ts --pkg",
    "build:single": "bun run ./scripts/build.ts --pkg",
    "build:old": "vite build && tsc",
    "build:types": "tsc --project tsconfig.build.json --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:parse": "bun run ./test/test.ts",
    "test:build": "bun run ./test/test-one.ts"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "looplan": "^1.3.17",
    "looplan-doc": "^1.0.6",
    "looplan-ui": "^1.5.24",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.16",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.3.2",
    "jsdom": "^26.1.0",
    "lodash-es": "^4.17.21",
    "sass": "^1.58.3",
    "typescript": "^5.8.2",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.1.1",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.8"
  },
  "files": [
    "lib",
    "types",
    "README.md",
    "LICENSE"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/loqhcn/looplan.git"
  },
  "main": "lib/mulo-open.umd.js",
  "module": "lib/looplan.es.js",
  "types": "types/index.d.ts"
}
